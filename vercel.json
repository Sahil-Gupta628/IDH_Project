{
    "builds": [
      {
        "src": "prescription_interpreter/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles_build/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "prescription_interpreter/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles_build"
  }